<IfModule mod_expires.c>
  ExpiresActive On

 # imgs
  ExpiresByType img/jpeg "access plus 1 year"
  ExpiresByType img/gif "access plus 1 year"
  ExpiresByType img/png "access plus 1 year"
  ExpiresByType img/webp "access plus 1 year"
  ExpiresByType img/svg+xml "access plus 1 year"
  ExpiresByType img/x-icon "access plus 1 year"

  # Video
  ExpiresByType video/webm "access plus 1 year"
  ExpiresByType video/mp4 "access plus 1 year"
  ExpiresByType video/mpeg "access plus 1 year"

  # Fonts
  ExpiresByType fonts/ttf "access plus 1 year"
  ExpiresByType fonts/otf "access plus 1 year"
  ExpiresByType fonts/woff "access plus 1 year"
  ExpiresByType fonts/woff2 "access plus 1 year"
  ExpiresByType application/font-woff "access plus 1 year"

  # CSS, JavaScript
  ExpiresByType css "access plus 1 year"
  ExpiresByType js "access plus 1 year"
  ExpiresByType application/js "access plus 1 year"

  # Others
  ExpiresByType application/pdf "access plus 1 year"
  ExpiresByType img/vnd.microsoft.icon "access plus 1 year"
</IfModule>